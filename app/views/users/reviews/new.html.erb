<h1>新規投稿記事</h1>
<!-- form_with部分 -->
<%= form_with model: @review, url: '/users/reviews', method: :post do |f| %>
  <h4>記事のタイトル</h4>
  <%= f.text_field :review_title %>

  <h4></h4>
  <%= f.text_field :title %>

  <h4>題材にした本の画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>本文</h4>
  <%= f.text_area :review_body %>

  <%= f.submit '投稿' %>
<% end %>
    <% users.each do |user| %>

      <%# if user.is_deleted == false %>

        <tbody>
          <tr>
        <td>
          <%= image_tag user.get_profile_image %>
        </td>
        <td>
          <%= user.name %>
        </td>
        <td><%= render "users/relationships/btn", user: user %></td>
        <td>フォロー数: <%= user.followings.count %></td>
        <td>フォロワー数: <%= user.followers.count %></td>
        <td>
          <%= link_to "Show", users_user_path(user.id) %>
        </td>
          </tr>
        </tbody>

      <%# end %>

    <% end %>

   </table>